<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.epson-pos.com/schemas/2011/03/epos-print" xmlns:tns="http://www.epson-pos.com/schemas/2011/03/epos-print" xmlns:xs="http://www.w3.org/2001/XMLSchema">

  
<!-- ===============================================================================================================
                                       S I M P L E   T Y P E S  -  S T A R T
     =========================================================================================================== -->


<!-- ============================================= R A N G E S ================================================= -->
  
  <!-- Standard Operator Range -->
  <xs:simpleType name="standardOperatorRange">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="12" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Standard Operator Range plus Second Range
  (+50 offset used with printRecItem, printRecRefund and printRecItemVoid. Regarding printRecItemVoid, the offset cannot be used if voidLastItem="1".
  Normally if the quantity = 1, the quantity line with "1x" is not printed.
  Adding the offset to the operator attribute instructs the fiscal printer to print the line when the quantity is exactly 1.
  The offset has no effect with direct invoices.
  -->

  <xs:simpleType name="operatorTwoRanges"> 
      <xs:union>
        <xs:simpleType>
          <xs:restriction base="xs:unsignedByte">
          <xs:minInclusive value="1" />
          <xs:maxInclusive value="12" />
          </xs:restriction>
        </xs:simpleType>
        <xs:simpleType>
          <xs:restriction base="xs:unsignedByte">
            <xs:minInclusive value="51" />
            <xs:maxInclusive value="62" />
          </xs:restriction>
        </xs:simpleType>
      </xs:union>
  </xs:simpleType>

  <!-- Department Range -->
  <xs:simpleType name="departmentRange">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Message Index Range Fiscal Receipts and Credit Notes -->
  <xs:simpleType name="messageIndexRangeFiscal">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="99" />
  </xs:restriction>
  </xs:simpleType>

  <!-- Message Index Range Non-fiscal Receipts -->
  <xs:simpleType name="messageIndexRangeNonFiscal">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="9" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Message Index Range Invoice -->
  <xs:simpleType name="messageIndexRangeInvoice">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="20" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Payment Index Range -->
  <xs:simpleType name="paymentIndexRange">
    <xs:restriction base="xs:unsignedByte">
     <xs:minInclusive value="0" />
      <xs:maxInclusive value="10" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Graphic Index Range-->
  <xs:simpleType name="graphicIndexRange">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="9" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Document Number Range-->
  <xs:simpleType name="documentNumberRange">
    <xs:restriction base="xs:unsignedInt">
     <xs:minInclusive value="0" />
      <xs:maxInclusive value="99999" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Retry Range-->
  <xs:simpleType name="retryRange">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="6000" />
      <xs:maxInclusive value="120000" />
    </xs:restriction>
  </xs:simpleType>

  <!-- EJ Number Range -->
  <xs:simpleType name="EJNumberRange">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="9999" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Timeout Range -->
  <xs:simpleType name="timeoutRange">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="3000" />
    </xs:restriction>
  </xs:simpleType>

  <!-- ========================== B O X    O F F I C E    T I C K E T    R A N G E S ============================= -->
  
  <!-- Random Number Range-->
  <xs:simpleType name="randomNumRange">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="00" />
      <xs:maxInclusive value="99" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Random Number Range-->
  <xs:simpleType name="controlCodeRange">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="0000" />
      <xs:maxInclusive value="9999" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- ============================================ L E N G T H S ================================================ -->
  
  <!-- Description Length Fiscal Receipt -->
  <xs:simpleType name="descriptionLengthFiscalReceipt">
    <xs:restriction base="xs:string">
      <xs:maxLength value="38" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Description Length -->
  <xs:simpleType name="descriptionLengthDirectInvoice">
    <xs:restriction base="xs:string">
      <xs:maxLength value="37" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="descriptionLengthPaymentDirectInvoice">
    <xs:restriction base="xs:string">
      <xs:maxLength value="20" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Data Display Length -->
  <xs:simpleType name="dataDisplayLength">
    <xs:restriction base="xs:string">
      <xs:maxLength value="40" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Data Display Length -->
  <xs:simpleType name="dataDisplayLengthBoxOfficeTicket">
    <xs:restriction base="xs:string">
      <xs:maxLength value="20" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Message Length -->
  <xs:simpleType name="messageLength">
    <xs:restriction base="xs:string">
      <xs:maxLength value="46" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Data Print Length -->
  <xs:simpleType name="dataPrintLength">
    <xs:restriction base="xs:string">
      <xs:maxLength value="46" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Data Print Length -->
  <xs:simpleType name="documentLineLength">
    <xs:restriction base="xs:string">
      <xs:maxLength value="46" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Data DirectIO Length -->
  <xs:simpleType name="dataDirectIOLength">
    <xs:restriction base="xs:string">
      <xs:maxLength value="501" />
    </xs:restriction>
  </xs:simpleType>


  <!-- ========================= B O X    O F F I C E    T I C K E T    L E N G T H S ============================ -->

  <!-- Text Length -->
  <xs:simpleType name="textLength">
    <xs:restriction base="xs:string">
      <xs:maxLength value="46" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- ============================================= F O R M A T S =============================================== -->
  
  <!-- Amount String Format with Zero allowed-->
  <xs:simpleType name="amountStringFormatWithZero">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1,7}" />
      <xs:pattern value="[0-9]{1,7}[.,]{1}[0-9]{1,2}"/>
      <xs:maxLength value="10" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Amount String Format with Zero not allowed -->
  <xs:simpleType name="amountStringFormatWithoutZero">
    <xs:restriction base="xs:string">
      <xs:pattern value="^[1-9]{1}[0-9]{0,6}"/>
      <xs:pattern value="^[1-9]{1}[0-9]{0,6}[.,]{1}[0-9]{1,2}"/>
      <xs:pattern value="^[0-9]{1}[1-9]{1}[0-9]{0,5}"/>
      <xs:pattern value="^[0-9]{1}[1-9]{1}[0-9]{0,5}[.,]{1}[0-9]{1,2}"/>
      <xs:pattern value="^[0-9]{2}[1-9]{1}[0-9]{0,4}"/>
      <xs:pattern value="^[0-9]{2}[1-9]{1}[0-9]{0,4}[.,]{1}[0-9]{1,2}"/>
      <xs:pattern value="^[0-9]{3}[1-9]{1}[0-9]{0,3}"/>
      <xs:pattern value="^[0-9]{3}[1-9]{1}[0-9]{0,3}[.,]{1}[0-9]{1,2}"/>
      <xs:pattern value="^[0-9]{4}[1-9]{1}[0-9]{0,2}"/>
      <xs:pattern value="^[0-9]{4}[1-9]{1}[0-9]{0,2}[.,]{1}[0-9]{1,2}"/>
      <xs:pattern value="^[0-9]{5}[1-9]{1}[0-9]{0,1}"/>
      <xs:pattern value="^[0-9]{5}[1-9]{1}[0-9]{0,1}[.,]{1}[0-9]{1,2}"/>
      <xs:pattern value="^[0-9]{6}[1-9]{1}"/>
      <xs:pattern value="^[0-9]{6}[1-9]{1}[.,]{1}[0-9]{1,2}"/>
      <xs:pattern value="^[0-9]{1,7}[.,]{1}[1-9]{1}[0-9]{0,1}" />
      <xs:pattern value="^[0-9]{1,7}[.,]{1}[0-9]{1}[1-9]{1}" />
      <xs:maxLength value="10" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Quantity String Format (zero not allowed) -->
  <xs:simpleType name="quantityStringFormat">
    <xs:restriction base="xs:string">  
      <xs:pattern value="^[1-9]{1}[0-9]{0,3}"/>
      <xs:pattern value="^[1-9]{1}[0-9]{0,3}[.,]{1}[0-9]{1,3}"/>
      <xs:pattern value="^[0-9]{1}[1-9]{1}[0-9]{0,2}"/>
      <xs:pattern value="^[0-9]{1}[1-9]{1}[0-9]{0,2}[.,]{1}[0-9]{1,3}"/>
      <xs:pattern value="^[0-9]{2}[1-9]{1}[0-9]{0,1}"/>
      <xs:pattern value="^[0-9]{2}[1-9]{1}[0-9]{0,1}[.,]{1}[0-9]{1,3}"/>
      <xs:pattern value="^[0-9]{3}[1-9]{1}"/>
      <xs:pattern value="^[0-9]{3}[1-9]{1}[.,]{1}[0-9]{1,3}"/>
      <xs:pattern value="^[0-9]{1,4}[.,]{1}[1-9]{1}[0-9]{0,2}" />
      <xs:pattern value="^[0-9]{1,4}[.,]{1}[0-9]{1}[1-9]{1}[0-9]{0,1}" />
      <xs:pattern value="^[0-9]{1,4}[.,]{1}[0-9]{2}[1-9]{1}" />
      <xs:maxLength value="8" />
      <xs:minLength value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Command Format (Direct IO) -->
  <xs:simpleType name="commandFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-5]{1}[0-9]{3}"/>
      <xs:minLength value="4" />
      <xs:maxLength value="4" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Day Format -->
  <xs:simpleType name="dayFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-9]{1}"/>
      <xs:pattern value="[0]{1}[1-9]{1}"/>
      <xs:pattern value="[1-2]{1}[0-9]{1}"/>
      <xs:pattern value="[3]{1}[0-1]{1}"/>
      <xs:minLength value="1" />
      <xs:maxLength value="2" />    
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="monthFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[1-9]{1}"/>
      <xs:pattern value="[0]{1}[1-9]{1}"/>
      <xs:pattern value="[1]{1}[0-2]{1}"/>
      <xs:minLength value="1" />
      <xs:maxLength value="2" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="yearFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{2}"/>
      <xs:pattern value="[2]{1}[0]{1}[0-9]{2}"/>
      <xs:minLength value="2" />
      <xs:maxLength value="4" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="hourFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1}"/>
      <xs:pattern value="[0-1]{1}[0-9]{1}"/>
      <xs:pattern value="[2]{1}[0-3]{1}"/>
      <xs:minLength value="1" />
      <xs:maxLength value="2" />
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="minuteFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1}"/>
      <xs:pattern value="[0-5]{1}[0-9]{1}"/>
      <xs:minLength value="1" />
      <xs:maxLength value="2" />
    </xs:restriction>
  </xs:simpleType>

  <!-- IP Address Format -->
  <xs:simpleType name="ipAddressFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}"/>
      <xs:maxLength value="15" />
      <xs:minLength value="7" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Graphic Formats -->
  <xs:simpleType name="graphicFormats">
    <xs:restriction base="xs:string">
      <xs:enumeration value="B" />
      <xs:enumeration value="R" />
    </xs:restriction>
  </xs:simpleType>

  <!-- EFTPOS Terminal ID Format -->
  <xs:simpleType name="terminalIDStringFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{8}" />
      <xs:maxLength value="8" />
      <xs:minLength value="8" />
    </xs:restriction>
  </xs:simpleType>

  <!-- EFTPOS Register ID Format -->
  <xs:simpleType name="registerIDStringFormat">
    <xs:restriction base="xs:string">
      <xs:pattern value="[0-9]{8}" />
      <xs:maxLength value="8" />
      <xs:minLength value="8" />
    </xs:restriction>
  </xs:simpleType>
  
<!-- ============================ T Y P E S  ,    F O N T S    O R    S I M I L A R =========================== -->
  
  <!-- Rec Item Adjustment Types -->
  <xs:simpleType name="recItemAdjustmentTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="0" />
      <xs:enumeration value="3" />
      <xs:enumeration value="5" />
      <xs:enumeration value="8" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Subtotal Adjustment Types -->
  <xs:simpleType name="subtotalAdjustmentTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
      <xs:enumeration value="6" />
      <xs:enumeration value="7" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Message Types -->
  <xs:simpleType name="fiscalReceiptMessageTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
      <xs:enumeration value="3" />
      <xs:enumeration value="4" />
      <xs:enumeration value="7" />
      <xs:enumeration value="8" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Message Types -->
  <xs:simpleType name="nonFiscalReceiptMessageTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="1" />
      <xs:enumeration value="8" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Message Types -->
  <xs:simpleType name="invoiceMessageTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="4" />
      <xs:enumeration value="5" />
      <xs:enumeration value="6" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Payment Types -->
  <xs:simpleType name="paymentTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
      <xs:enumeration value="3" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Data Types -->
  <xs:simpleType name="EJDataTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
      <xs:enumeration value="3" />
      <xs:enumeration value="4" />
      <xs:enumeration value="5" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Document Types -->
  <xs:simpleType name="documentTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="lastReceiptInvoice" />
      <xs:enumeration value="directInvoice" />
      <xs:enumeration value="freeInvoice" />
      <xs:enumeration value="receiptBasedFreeInvoice" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Operation Types (used with endFiscalDocument -->
  <xs:simpleType name="operationTypes">
    <xs:restriction base="xs:unsignedByte">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Device Types (used with fPMateConfiguration -->
  <xs:simpleType name="deviceTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="fpPrinter" />
      <xs:enumeration value="tmPrinter" />
    </xs:restriction>
  </xs:simpleType>
  

  <!-- Justifications -->
  <xs:simpleType name="justifications">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="2" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Fonts (includes box office ticket fonts)-->
  <xs:simpleType name="fonts">
    <xs:restriction base="xs:string">
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
      <xs:enumeration value="3" />
      <xs:enumeration value="4" />
      </xs:restriction>
  </xs:simpleType>

  <!-- Options -->
  <xs:simpleType name="options">
    <xs:restriction base="xs:string">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Logo Options -->
  <xs:simpleType name="logoOptions">
    <xs:restriction base="xs:string">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Logo Location Options -->
  <xs:simpleType name="logoLocationOptions">
    <xs:restriction base="xs:string">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
      <xs:enumeration value="3" />
      <xs:enumeration value="4" />
    </xs:restriction>
  </xs:simpleType>
  
  <!-- Directions -->
  <xs:simpleType name="directions">
    <xs:restriction base="xs:string">
      <xs:enumeration value="in" />
      <xs:enumeration value="out" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Forms -->
  <xs:simpleType name="forms">
    <xs:restriction base="xs:string">
      <xs:enumeration value="cash" />
      <xs:enumeration value="cheque" />
      <xs:enumeration value="check" />
    </xs:restriction>
  </xs:simpleType>

  <!-- clearEFTPOSBufferOptions -->
  <xs:simpleType name="clearEFTPOSBufferOptions">
  <xs:restriction base="xs:string">
    <xs:enumeration value="0" />
    <xs:enumeration value="1" />
  </xs:restriction>
  </xs:simpleType>

  <!-- Status Types -->
  <xs:simpleType name="statusTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
    </xs:restriction>
  </xs:simpleType>

  <!-- ========================== B O X    O F F I C E    T I C K E T    T Y P E S  ============================ -->

  <!-- textTypes -->
  <xs:simpleType name="textTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="R" />
      <xs:enumeration value="S" />
    </xs:restriction>
  </xs:simpleType>

  <!-- ===============================================================================================================
                                         F I S C A L   B A R   C O D E S  -  S T A R T
     =========================================================================================================== -->

  <!-- Position Range (non QR codes only)-->
  <xs:simpleType name="positionRange">
    <xs:restriction base="xs:unsignedInt">
      <xs:minInclusive value="0" />
      <xs:maxInclusive value="511" />
      <xs:enumeration value="900" />
      <xs:enumeration value="901" />
      <xs:enumeration value="902" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Width Range (non QR codes only)-->
  <xs:simpleType name="widthRange">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="6" />
    </xs:restriction>
  </xs:simpleType>

  <!-- Height Range (non QR codes only) -->
  <xs:simpleType name="heightRange">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="255" />
    </xs:restriction>
  </xs:simpleType>

  <!-- HRI Positions  (non QR codes only) (0 = Disabled, 1 = below, 2 = above and 3 = below and above) -->
  <xs:simpleType name="hRIPositions">
    <xs:restriction base="xs:string">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
      <xs:enumeration value="3" />
    </xs:restriction>
  </xs:simpleType>

  <!-- HRI Fonts (non QR codes only) -->
  <xs:simpleType name="hRIFonts">
    <xs:restriction base="xs:string">
      <xs:enumeration value="A" />
      <xs:enumeration value="B" />
      <xs:enumeration value="C" />
    </xs:restriction>
  </xs:simpleType> 

  <!-- qRCodeAlignments  (0 = left, 1 = centred and 2 = right) -->
  <xs:simpleType name="qRCodeAlignments">
    <xs:restriction base="xs:string">
      <xs:enumeration value="0" />
      <xs:enumeration value="1" />
      <xs:enumeration value="2" />
    </xs:restriction>
  </xs:simpleType>
    
  <!-- QR Code Size Range -->
  <xs:simpleType name="qRCodeSizeRange">
    <xs:restriction base="xs:unsignedByte">
      <xs:minInclusive value="1" />
      <xs:maxInclusive value="16" />
    </xs:restriction>
  </xs:simpleType>

  <!-- QR Code Data Types -->
  <xs:simpleType name="qRCodeDataTypes">
    <xs:restriction base="xs:string">
      <xs:enumeration value="alphaNumeric" />
      <xs:enumeration value="binary" />
      </xs:restriction>
  </xs:simpleType>

